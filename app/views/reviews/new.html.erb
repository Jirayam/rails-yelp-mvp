<div class="container">
<h1 class="text-center">Add a new review to <% @restaurant %></h1>
<%= simple_form_for [@restaurant, @review] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating, collection: Review::RATINGS %>
  <%= f.submit "add a review", class: "btn btn-primary" %>
<% end %>
</div>
